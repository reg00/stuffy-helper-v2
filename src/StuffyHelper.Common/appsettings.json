{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Information"
    }
  },
  "StuffyHelper": {
    "Endpoints": {
      "EmailEndpoint": "<email_service_endpoint>",
      "AuthorizationEndpoint": "<authorization_service_endpoint>"
    },
    "MinioStore": {
      "Endpoint": "<minio_endpoint>",
      "AccessKey": "<minio_access_key>",
      "SecretKey": "<minio_secret_key>",
      "NetworkTimeout": 3600,
      "PresignedUrlExpiry": "12:00:00"
    },
    "FileStore": {
      "ContainerName": "stuffyblob"
    },
    "EntityFramework": {
      "DbProvider": "PostgreSql",
      "ConnectionString": "Server=*;Database=stuffy-helper;User Id=*;Password=*;"
    },
    "Authorization": {
      "ConnectionString": "Server=*;Database=stuffy-helper-auth;User Id=*;Password=*;",
      "JWT": {
        "ValidAudience": "audience",
        "ValidIssuer": "issuer",
        "Secret": "secret",
        "TokenExpireInHours": "24"
      }
    },
    "EmailService": {
      "Server": "<email_server>",
      "Port": "<port>",
      "Account": "<account>",
      "Password": "<password>",
      "SenderEmail": "<sender_email>",
      "SenderName": "<sender_name>"
    },
    "CheckFinderClient": {
      "Endpoint": "<check_finder_service>"
    }
  },
  "AllowedHosts": "*",
  "Frontend": {
    "Endpoint": "http:\\....."
  }
}