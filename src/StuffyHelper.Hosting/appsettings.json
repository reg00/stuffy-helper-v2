{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning",
      "System.Net.Http.HttpClient": "Information",
      "Microsoft.AspNetCore.Hosting": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "MinioStore": {
    "Endpoint": "localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "NetworkTimeout": 3600,
    "PresignedUrlExpiry": "12:00:00"
  },
  "StuffyFileWeb": {
    "StuffyFileStore": {
      "ContainerName": "rawfilewebcontainer"
    }
  },
  "EntityFramework": {
    "DbProvider": "PostgreSql",
    "ConnectionString": "Server=*;Database=stuffy-helper-v2;User Id=*;Password=*;"
  },
  "Authorization": {
    "ConnectionString": "Server=*;Database=stuffy-helper-auth-v2;User Id=*;Password=*;",
    "JWT": {
      "ValidAudience": "audience",
      "ValidIssuer": "issuer",
      "Secret": "secret",
      "TokenExpireInHours": "24"
    }
  },
  "AllowedHosts": "*"
}
