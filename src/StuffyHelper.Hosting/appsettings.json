{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning",
      "System.Net.Http.HttpClient": "Information",
      "Microsoft.AspNetCore.Hosting": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "MinioStore": {
    "Endpoint": "<minio_endpoint>",
    "AccessKey": "<minio_access_key>",
    "SecretKey": "<minio_secret_key>",
    "NetworkTimeout": 3600,
    "PresignedUrlExpiry": "12:00:00"
  },
  "FileStore": {
    "ContainerName": "stuffyblob"
  },
  "EntityFramework": {
    "DbProvider": "PostgreSql",
    "ConnectionString": "Server=*;Database=stuffy-helper;User Id=*;Password=*;"
  },
  "Authorization": {
    "ConnectionString": "Server=*;Database=stuffy-helper-auth;User Id=*;Password=*;",
    "JWT": {
      "ValidAudience": "audience",
      "ValidIssuer": "issuer",
      "Secret": "secret",
      "TokenExpireInHours": "24"
    }
  },
  "EmailService": {
    "Server": "<email_server>",
    "Port": "<port>",
    "Account": "<account>",
    "Password": "<password>",
    "SenderEmail": "<sender_email>",
    "SenderName": "<sender_name>"
  },
  "AllowedHosts": "*",
  "Frontend": {
    "Endpoint": "http:\\....."
  }
}
